<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.BerryGoodSmoothies" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#58b19f</item>
        <item name="colorPrimaryVariant">#58b19f</item>
        <item name="colorOnPrimary">@color/black</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/pcY</item>
        <item name="colorSecondaryVariant">@color/pcY</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
<!--        <item name="android:fontFamily">@font/baloo</item>-->
<!--        <item name="fontFamily">@font/baloo</item>-->
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.BerryGoodSmoothies.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.BerryGoodSmoothies.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.BerryGoodSmoothies.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>